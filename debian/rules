#!/usr/bin/make -f
# See debhelper(7) (uncomment to enable)
# output every command that modifies files on the build system.
#export DH_VERBOSE = 1

%:
	dh $@ --with systemd


override_dh_installinit:
	dh_installinit --name=platina-mk1-ttys
	dh_installinit --name=platina-mk1-ttys0

override_dh_systemd_enable:
	dh_systemd_enable --name=platina-mk1-ttys
	dh_systemd_enable --name=platina-mk1-ttys0

override_dh_systemd_start:
	dh_systemd_start --name=platina-mk1-ttys --no-stop-on-upgrade --no-restart-after-upgrade
	dh_systemd_start --name=platina-mk1-ttys0 --no-stop-on-upgrade --no-restart-after-upgrade
